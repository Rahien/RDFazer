
{
    "name": "RDFazer",
    "version": "0.1",
    "manifest_version": 2,
    "description": "Tag concepts on a page with RDFa",
    "browser_action": {
	"default_icon": "icon.png"
    },
    "background": {
        "scripts":["lib/jquery-2.0.3.min.js","background.js"]
    },
    "permissions": ["tabs", "<all_urls>","storage", "http://*/*", "https://*/*", "https://*/"],
    "web_accessible_resources": ["lib/jquery-2.0.3.min.js","contentscript.css","settings.html", "interface.html","dialog.html","lib/jquery-ui.css","lib/images/*"]
}
